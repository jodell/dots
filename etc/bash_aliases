alias ll='ls -l'
alias la='ls -a'
alias l='ls -CF'
alias h='history'
alias go="cd ~/git"
alias dots="cd ~/git/dotsfiles"
alias rgrep="grep -r"
alias ffp='/Applications/Firefox.app/Contents/MacOS/firefox-bin -profilemanager'

# SO META
